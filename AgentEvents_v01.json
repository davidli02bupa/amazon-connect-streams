          "rawQuery": "With \nInstanceBaseQuery AS (\n    SELECT InstanceAlias, Arn, ROW_NUMBER() OVER (PARTITION BY \"Arn\" ORDER BY time desc) AS seqnum \n    FROM \"ConnectRealTime\".\"Instance\" \n    GROUP BY time, InstanceAlias, Arn\n),\nInstanceAliasQuery AS (\n    SELECT InstanceAlias, Arn FROM InstanceBaseQuery\n    WHERE seqnum =1\n), \nBaseAvailabilityByDate AS (\nSELECT \nBIN(time, greatest($__interval_ms,5m)) binned_timestamp,\nAgentARN,\nMax(CASE\n    WHEN CurrentAgentSnapshot_AgentStatus_Name = 'Available' THEN 1\n    ELSE 0\nEND) \"Max Availability\",\nMin(CASE\n    WHEN CurrentAgentSnapshot_AgentStatus_Name = 'Available' THEN 1\n    ELSE 0\nEND) \"Min Availability\"\nFROM \"$DatabaseName\".\"AgentEvent\" eventsource\n    LEFT JOIN InstanceAliasQuery instance\n    ON eventsource.InstanceARN = instance.Arn\nWhere \n    time BETWEEN from_milliseconds(${__from}) AND from_milliseconds(${__to}) \n    AND \"measure_name\" = 'AgentEvent' \n    AND instance.InstanceAlias IN ( $InstanceAlias )\nGroup by BIN(time, greatest($__interval_ms,5m)) , AgentARN\n),\nSumAvailabilityByDate AS (\nSELECT \nbinned_timestamp,\nsum (\"Max Availability\") as \"Max Number of Available Agents\",\nsum (\"Min Availability\") as \"Min Number of Available Agents\"\nFROM BaseAvailabilityByDate\nGROUP BY binned_timestamp\n)\nSELECT \nCREATE_TIME_SERIES (binned_timestamp,\"Max Number of Available Agents\") \"Max Number of Available Agents\",\nCREATE_TIME_SERIES (binned_timestamp,\"Min Number of Available Agents\") \"Min Number of Available Agents\"\nFROM SumAvailabilityByDate",
